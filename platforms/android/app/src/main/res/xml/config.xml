<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="com.wGuiGhostFarms_9861566" id="com.wGuiGhostFarms_9861566" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Medieval Farms</name>
    <description>
       Retro Farming Sim
    </description>
